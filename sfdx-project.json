{
  "packageDirectories": [
    {
      "path": "examples",
      "default": true
    },
    {
      "path": "package"
    }
  ],
  "replacements": [
    {
      "glob": "package/main/default/classes/*.cls",
      "stringToReplace": "ENV_PREFIX_",
      "replaceWithEnv": "SF_ENV_PREFIX",
      "allowUnsetEnvVariable": true
    }
  ],
  "name": "apex-proxy",
  "namespace": "proxy",
  "sfdcLoginUrl": "https://login.salesforce.com",
  "sourceApiVersion": "61.0"
}
